@leadarticle{omoruyi2024beads,
  title={Beads-on-a-string Star Formation Tied to One of the Most Powerful Active Galactic Nucleus Outbursts Observed in a Cool-core Galaxy Cluster},
  author={Omoruyi, O. and Tremblay, G. R. and Combes, F. and Davis, T. A. and Gladders, M. D. and Vikhlinin, A. and Nulsen, P. and Kharb, P. and Baum, S. A. and O’Dea, C. P. and Sharon, K. and Terrazas, B. A. and Nevin, R. and Schechter, A. L. and Zuhone, J. A. and McDonald, M. and Dahle, H. and Bayliss, M. B. and Connor, T. and Florian, M. and Rigby, J. R. and Vaddi, S.},
  year={2024},
  journal=apj,
  volume={963},
  number={1},
  numpages={30},
  pdf={omo_beads_2312.06762v2.pdf},
  preview={beads.gif},
  type={lead},
  video={https://www.youtube.com/watch?v=blvcW0M-REU},
  html={https://ui.adsabs.harvard.edu/abs/2024ApJ...963....1O/abstract},
  abstract={Using high-resolution multi-wavelength data from Chandra, HST, ALMA, and LOFAR, we discovered a chain of 19 young stellar “beads” connected to a giant X-ray cavity in an intermediate-redshift cool-core cluster. We show that this star formation pattern likely arose from gas cooling in the turbulent core, a powerful AGN outburst displacing cold gas, and tidal interactions from merging galaxies. These results highlight how feedback, cooling, and mergers interact in complex ways, offering insight into the chaotic processes that shaped galaxies and clusters in the early Universe.}
}

@leadarticle{omoruyi2025a2597,
  title={A 600 ks Chandra View of Abell 2597},
  author={Omoruyi, O. and Tremblay, G. and Vikhlinin, A. and Dabhade, P. and Raychaudhury, S. and Markevitch, M. and Zuhone, J. and Nulsen, P. and Baum, S. A. and O’Dea, C. P. and Clarke, T. and Randall, S. and Kharb, P. and Gulati, S. and Vaddi, S.},
  year={2025},
  journal=apj,
  preview={a2597_chandra.png},
  pdf={omo_a2597.pdf},
  type={lead},
  additional_info={*Accepted to ApJ. Expected on arXiv soon*},
  abstract={Using deep $\sim$600 ks Chandra observations with archival GMRT and SINFONI data, we mapped the impact of repeated AGN outbursts in Abell 2597, revealing seven cavities, weak shocks to $\sim$150 kpc, and a 57 kpc surface brightness deficit likely shaped by sloshing-driven magnetic amplification. We show that although the AGN injects energy comparable to the cooling luminosity, radiative cooling persists at $\sim$15 $M_\odot$ yr$^{-1}$, fueling a massive cold gas reservoir that Bondi accretion cannot supply. These results support chaotic cold accretion as the dominant fueling mode and provide a detailed view of how cooling, feedback, and gas dynamics regulate cool-core cluster evolution.}
}

@leadunpublished{omoruyi2026tngsam,
  title={Emulating the IllustrisTNG Simulation with a Semi-analytic Model},
  author={Omoruyi, O. and Terrazas, B. and Gabrielpillai, A. and Oren, Y. and Pandya, V. and Somerville, R. and Hernquist, L.},
  year={2026},
  journal=mnras,
  preview={tngsam.png},
  pdf={tngsam.pdf},
  type={lead},
  additional_info={*Submitted to MNRAS*},
  abstract={By combining the strengths of the Santa Cruz semi-analytic model and the IllustrisTNG100 simulation, we developed the TNG SAM, a fast model calibrated to reproduce TNG’s gas and metal flows in stellar-feedback–dominated galaxies. We show that physically motivated updates to cooling, star formation, recycling, outflows, and gas re-accretion allow the SAM to recover TNG’s baryon cycle and global galaxy properties to within $\sim$30\% out to $z=6$. This work demonstrates that well-calibrated SAMs can capture much of the complex physics of hydrodynamical simulations while enabling efficient parameter studies across cosmological volumes.}
}

